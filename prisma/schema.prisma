datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id    String  @id
  name  String?
  email String  @unique

  groups Group[]
}

model Group {
  id    String @id
  title String @unique

  members User[]
}
